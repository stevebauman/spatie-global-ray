{
    "name": "stevebauman/spatie-global-ray",
    "description": "Install Spatie Ray globally.",
    "keywords": [
        "spatie",
        "ray",
        "global"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steve_bauman@outlook.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "titasgailius/terminal": "^1.1",
        "symfony/console": "^4.0|^5.0|^6.0",
        "symfony/process": "^4.2|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "bin": [
        "bin/ray"
    ],
    "autoload": {
        "psr-4": {
            "Stevebauman\\SpatieGlobalRay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stevebauman\\SpatieGlobalRay\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
